{
  "info": {
    "name": "Skiptest API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Create Skiptest",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"{{name}}\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{base_url}}/api/v1/skiptest",
          "host": [
            "{{base_url}}"
          ],
          "path": [
            "api",
            "v1",
            "skiptest"
          ]
        }
      },
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "pm.variables.set(\"name\", \"name_\" + Date.now());"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
